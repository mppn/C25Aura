int i,j,k;
int a[2][2]= {{2,3},{4,6}};
int b[2][2]={{7,3},{8,2}};
int c[2][2];

    for(i=0;i<=1;i++)
    {
        for(j=0;j<=1;j++)
        {
            c[i][j]=0;
            for(k=0;k<=1;k++)
            {
                 c[i][j]+=a[i][k]*b[k][j];   
               
            }
            printf("%d\t",c[i][j]);
        }
        
    printf("\n");
    }
