#include <stdio.h>

int main()
{
int i,j;
int a[3][3]={{2,3,5},{4,6,5},{3,6,8}};
int b[3][3]={{7,3,8},{8,2,4},{2,4,6}};
int c[3][3];
    for(i=0;i<=2;i++)
    {
        for(j=0;j<=2;j++)
        {
            c[i][j]=a[i][j]+b[i][j];          
               printf("%d\t",c[i][j]);
         }
      printf("\n");
    }
return 0;
}
