
#include <stdio.h>

int main()
{
int i,j,t,n=5;
int a[5]={2,3,7,8,5};
{
    for(i=0;i<=n-1;i++)
    {
        for(j=i+1;j<=n-1;j++)
        {
            if(a[i]<a[j])
            {
            t=a[i];
            a[i]=a[j];
            a[j]=t;
            }
            
        }   
       printf("%d",a[i]); 
    }
   
}
    return 0;
}
